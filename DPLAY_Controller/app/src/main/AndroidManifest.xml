<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hun.myapplication">

    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LEDActivity"
            android:label="LED">
        </activity>

        <activity
            android:name=".SensorActivity"
            android:label="Sensor">
        </activity>

        <activity
            android:name=".BuzzerActivity"
            android:label="Buzzer">
        </activity>

        <activity
            android:name=".MotorActivity"
            android:label="Motor">
        </activity>

        <activity
            android:name=".ButtonActivity"
            android:label="Button/Tilt">
        </activity>

        <activity
            android:name=".SettingsActivity"
            android:label="Settings">
        </activity>

        <activity
            android:name=".DeviceListActivity"
            android:label="Device List">
        </activity>
    </application>

</manifest>